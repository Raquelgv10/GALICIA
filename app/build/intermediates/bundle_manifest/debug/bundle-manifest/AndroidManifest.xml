        <activity android:name="com.example.galicia.Coruna" />
        <activity android:name="com.example.galicia.Ourense" />
        <activity android:name="com.example.galicia.Pontevedra" />
        <activity android:name="com.example.galicia.ItemDetallado" />
        <activity android:name="com.example.galicia.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>